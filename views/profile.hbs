<body>
    <div class="">
      <a href="/">Log Out</a>
      <a href="/edit-profile/{{newest.username}}">Edit Profile</a>
    </div>
    <div class="container toplevel">
      <div class="row col-sm-12">
        <div class="col-sm-3">
          <img style="height:200px;" src="/images/front-piano.jpg" alt="">
        </div>
        <div class="col-sm-9">
          <img style="height:200px;"src="/images/wooden-music-box.jpg" alt="">
        </div>
      </div>
    </div>
    <main>
      <div class="container biosongs">
        <div class="row col-sm-12">
          <div class="col-sm-3 bio" style="border:2px solid black;">
            <h2> Welcome, {{newest.first_name}} {{newest.last_name}}</h2>
            <h4>Bio</h4>
            <p> {{newest.bio}} </p>
            <h4>Favorite Genre</h4>
            <p>{{newest.fav_genre}}</p>
            <h4>Biggest Musical Influences</h4>
            <p>{{newest.influences}}
            <h4>Team</h4>
            <p>{{newest.team}}</p>
          </div>
          <div class="col-sm-9 songs" style="border:2px solid black;">
            <div class="songtopbar">
              <h2> Put on some of that {{newest.fav_genre}}!</h2>
              <h2><button style="justify-content:flex-end;"><a href="/create-song/{{newest.username}}">New Song</a></button></h2>
            </div>
            <div class="">
              <figure>
                SONG 1
              </figure>
              <figure>
                SONG 2
              </figure>
              <figure>
                SONG 3
              </figure>
            </div>
          </div>
        </div>
      </div>
    </main>













